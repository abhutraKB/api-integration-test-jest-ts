{
  "name": "biz-analyst-api-test",
  "version": "1.0.0",
  "description": "testing biz analyst apis",
  "main": "index.js",
  "scripts": {
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch"
  },
  "author": "Ashish Bhutra",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^27.0.3",
    "@types/supertest": "^2.0.11",
    "codelyzer": "6.0.2",
    "jest": "^27.4.4",
    "node": "17.2.0",
    "supertest": "^6.1.6",
    "ts-jest": "^27.1.1",
    "tslint": "6.1.3",
    "typescript": "4.5.3",
    "json-object-mapper": "1.7.1",
    "reflect-metadata": "0.1.13",
    "faker": "5.5.3",
    "@types/faker": "5.5.9"
  },
  "dependencies": {
    "@jest/test-sequencer": "27.4.5"
